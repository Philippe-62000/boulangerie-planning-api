RewriteEngine On 
 
# Pour la racine 
RewriteCond %{REQUEST_FILENAME} !-f 
RewriteCond %{REQUEST_FILENAME} !-d 
RewriteRule (.*)$ index.html [L] 
 
# Pour /plan/ 
RewriteCond %{REQUEST_URI} /plan/ 
RewriteCond %{REQUEST_FILENAME} !-f 
RewriteCond %{REQUEST_FILENAME} !-d 
RewriteRule (.*)$ /plan/index.html [L] 
